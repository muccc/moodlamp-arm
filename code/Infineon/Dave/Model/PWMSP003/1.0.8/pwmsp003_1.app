er) / Math.log( 2 ) );&#xD;&#xA;                     SCM.setIntValue(Res3, Prescaler);&#xD;&#xA;                 }&#xD;&#xA;&#xD;&#xA;                function BackwardMapping(){&#xD;&#xA;                    var tempRes = SCM.getResource(&quot;pwmsp003_tempresolution&quot;);&#xD;&#xA;                    var value = SCM.getDoubleValue(tempRes);&#xD;&#xA;                    SCM.setDoubleValue(currentResource,value);&#xD;&#xA;&#xD;&#xA;                    var Res2 = SCM.getResource(&quot;ccu4globalapp/ccu4global_irclock&quot;);&#xD;&#xA;                    var Clock = SCM.getDoubleValue(Res2);&#xD;&#xA;&#xD;&#xA;                    var clkDiv = (1000 /Clock);&#xD;&#xA;&#xD;&#xA;                    var res_max=Math.ceil(clkDiv*32768);&#xD;&#xA;                    var res_min=Math.ceil(clkDiv);&#xD;&#xA;&#xD;&#xA;                    SCM.setMinMaxValue(currentResource,res_min,res_max,1);&#xD;&#xA;                }&#xD;&#xA;                " URI="http://www.infineon.com/1.0.8/app/pwmsp003/1/pwmsp003_irwresolution" downWardmappedList="//@consumed.1 //@consumed.52" maxValue="7d000" minValue="10">
